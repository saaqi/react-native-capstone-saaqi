{
  "expo": {
    "name": "Little Lemon",
    "slug": "little-lemon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6FCDF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#F6FCDF"
      },
      "package": "com.saqibtech.littlelemon"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Karla-Bold.ttf",
            "./assets/fonts/Karla-ExtraBold.ttf",
            "./assets/fonts/Karla-Medium.ttf",
            "./assets/fonts/Karla-Regular.ttf",
            "./assets/fonts/MarkaziText-Medium.ttf",
            "./assets/fonts/MarkaziText-Regular.ttf"
          ]
        }
      ],
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "0c41aec0-cd35-4b4e-b282-0051921bff90"
      }
    }
  }
}
